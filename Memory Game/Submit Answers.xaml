<Window x:Class="Memory_Game.Submit_Answers"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Memory_Game"
        mc:Ignorable="d"
        Title="Submit_Answers" Height="300" Width="300" WindowStartupLocation="CenterScreen">
    <Grid x:Name="Menu" Margin="0,0,2,0">
        <Label Content="Memory Game" HorizontalAlignment="Left" Margin="27,10,0,0" VerticalAlignment="Top" Width="251" FontSize="36" Foreground="White"/>
        <Button x:Name="button" Content="Button" HorizontalAlignment="Left" Margin="88,149,0,0" VerticalAlignment="Top" Width="116" Height="22" Click="button_Click_1"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="88,88,0,0" TextWrapping="Wrap" Text="TextBox" VerticalAlignment="Top" Width="120"/>
    </Grid>
</Window>
